    "babel-loader": "9.1.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "browserslist": "^4.23.0",
    "buffer": "^6.0.3",
    "camelcase": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "^6.10.0",
    "date-holidays": "^3.23.10",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "events": "^3.3.0",
    "express": "^4.19.2",
    "file-loader": "6.2.0",
    "fomantic-ui-css": "^2.9.3",
    "fs-extra": "11.2.0",
    "html-webpack-plugin": "5.6.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "mini-css-extract-plugin": "2.8.1",
    "pnp-webpack-plugin": "1.7.0",
    "postcss": "^8.4.38",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.5.2",
    "postcss-safe-parser": "^7.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-iframe": "^1.8.5",
    "react-prerendered-component": "^1.2.5",
    "react-router-dom": "^6.22.3",
    "resolve": "1.22.8",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "14.1.1",
    "semantic-ui-react": "^2.1.5",
    "semver": "^7.6.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.10",
    "ts-pnp": "1.2.0",
    "url": "^0.11.3",
    "url-loader": "4.1.1",
    "webpack": "5.91.0",
    "webpack-dev-server": "^5.0.4",
    "workbox-webpack-plugin": "7.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && yarn postbuild",
    "test": "node scripts/test.js",
    "predeploy": "npm run build",
    "postbuild": "react-snap && react-snap-sitemap --base-url='https://gingerbuffet.com/' --change-frequency=daily",
    "deploy": "yarn predeploy && gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint-webpack-plugin": "^4.1.0",
    "fomantic-ui-less": "^2.9.3",
    "gh-pages": "^6.1.1",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "react-snap": "^1.23.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-nano": "^1.1.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "packageManager": "yarn@4.1.1"
}
