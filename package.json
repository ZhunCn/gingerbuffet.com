{
  "name": "gingerbuffet.com",
  "homepage": "https://gingerbuffet.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@artsy/fresnel": "^7.1.4",
    "@babel/core": "7.24.3",
    "@leadhome/react-snap-sitemap": "^0.1.4",
    "@loadable/component": "^5.16.3",
    "@svgr/webpack": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
